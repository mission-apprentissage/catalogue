{
    "comments": false,
    "presets": [
        [ 
            "@babel/preset-env",
            {
                "targets": {
                    "node": "12.11"
                }
             }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-object-rest-spread",
        "@babel/plugin-proposal-export-default-from",
        ["@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
        ]
    ]
}