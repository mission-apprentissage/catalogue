{
  "root": true,
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "env": {
    "node": true,
    "browser": false
  },
  "rules": {
    "import/prefer-default-export": [0],
    "no-console": [0, "log", "error"],
    "no-underscore-dangle": ["error", { "allow": ["__item"] }]
  }
}
